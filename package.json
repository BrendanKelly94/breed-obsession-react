{
  "name": "breed-obsession-react",
  "version": "1.0.0",
  "description": "React implementation on breed-obsession built with angular 5",
  "main": "lib/server.js",
  "author": "Brendan Kelly",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node --name appDev",
    "webpack": "webpack -wd",
    "test": "jest --watch",
    "verify-tests": "jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start": "NODE_ENV=production NODE_PATH=./build node build/server.js",
    "heroku-postbuild": "yarn build-node && yarn build-webpack"
  },
  "private": false,
  "babel": {
    "presets": [
      "react",
      ["env", {"targets": {"node": "current"} }],
      "stage-2"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-sass-export": "^0.0.3",
    "css-loader": "^0.28.11",
    "enzyme": "^2.9.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-jsonp": "^1.1.3",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "pm2": "^2.10.3",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reduxsauce": "^0.7.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^3.2.0"
  },
  "jest":{
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  }
}
